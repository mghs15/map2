{
  "version": 8,
  "name": "Vector",
  "metadata": {},
  "sources": {
    "bv": {
      "type": "vector",
      "tiles": [
      "https://cyberjapandata.gsi.go.jp/xyz/experimental_bvmap/{z}/{x}/{y}.pbf"
      ],
      "maxzoom": 18,
      "minzoom": 4,
      "attribution": "地理院地図Vector（仮称）提供実験"
    },
    "raster": {
      "maxzoom": 8,
      "tileSize": 256,
      "tiles": [
        "https://cyberjapandata.gsi.go.jp/xyz/earthhillshade/{z}/{x}/{y}.png"
      ],
      "type": "raster",
      "attribution": "地理院タイル（陰影起伏図）"
    }
  },
  "sprite": "https://maps.gsi.go.jp/vector/sprite/std",
  "glyphs": "https://maps.gsi.go.jp/xyz/noto-jp/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "rgb(255,255,255)"
      }
    },
    {
      "id": "raster",
      "type": "raster",
      "source": "raster",
      "maxzoom": 6,
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "raster-opacity": 0.3
      }
    },
    {
      "id": "waterarea",
      "type": "fill",
      "source": "bv",
      "source-layer": "waterarea",
	  "paint": {
	    "fill-color": "rgba(200, 201, 232, 1)",
	    "fill-opacity": 0.7,
	    "fill-outline-color": "rgba(95, 107, 208, 1)"
	  }
    },
    {
      "id": "coastline",
      "type": "line",
      "source": "bv",
      "source-layer": "coastline",
      "paint": {
        "line-color": "rgba(70, 138, 179, 1)"
      }
    },
    {
      "id": "river",
      "type": "line",
      "source": "bv",
      "source-layer": "river",
      "paint": {
        "line-color": "rgba(21, 95, 175, 1)"
      }
    },
    {
      "id": "railway",
      "type": "line",
      "source": "bv",
      "source-layer": "railway",
      "paint": {
        "line-width": 4,
        "line-color": "rgba(21, 95, 175, 1)"
      }
    },
    {
      "id": "railway-outline",
      "type": "line",
      "source": "bv",
      "source-layer": "railway",
      "paint": {
        "line-width": 2,
        "line-color": "rgba(255, 255, 255, 1)",
        "line-dasharray": [3, 3]
      }
    },
    {
      "id": "road",
      "type": "line",
      "source": "bv",
      "source-layer": "road",
      "paint": {
        
        "line-color": "rgba(175, 21, 21, 1)"
      }
    },
    {
      "id": "boundary",
      "type": "line",
      "source": "bv",
      "source-layer": "boundary",
      "paint": {
        "line-color": "rgba(114, 21, 175, 1)",
            "line-dasharray": [
		      3,
		      3
		    ]
      }
    },
    {
      "id": "symbol",
      "type": "symbol",
      "source": "bv",
      "source-layer": "symbol",
      "layout": {
        "text-field": "{knj}",
        "text-size": 15,
        "text-anchor": "left",
        "text-offset": [2,0],
        "text-font":["NotoSansCJKjp-Regular"],
        "icon-image": "{人口100万人以上2-500}"
      },
      "paint": {
        "text-color": "rgba(0,0,0,1)",
        "text-halo-color": "rgba(255,255,255,1)",
        "text-halo-width": 2
      }
    },
    {
      "id": "elevation",
      "type": "symbol",
      "source": "bv",
      "source-layer": "elevation",
      "layout": {
        "text-field": "{alti}",
        "text-size": 10,
        "text-anchor": "left",
        "text-offset": [1,0],
        "text-font":["NotoSansCJKjp-Regular"],
        "icon-image": "{標高点（測点）}"
      },
      "paint": {
        "text-color": "rgba(0,0,0,1)",
        "text-halo-color": "rgba(255,255,255,1)",
        "text-halo-width": 1
      }
    }
  ],
  "id": "gsi-bv"
}